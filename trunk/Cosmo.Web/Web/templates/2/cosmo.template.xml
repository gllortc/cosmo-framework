      <controls>

         <!-- CABECERA DE DOCUMENTO -->
         <pcontrol id="CosmoUiCtrlHeader" center="true" lateral="false">
            <!--<cscript type="external" src="http://static.addtoany.com/menu/page.js" />-->
            <celement id="page-title">
&lt;h3&gt;[@TITLE]&lt;/h3&gt;
[#IF DESCRIPTION]&lt;blockquote class="small"&gt;
   &lt;p&gt;[@DESCRIPTION][#IF AUTHOR]&lt;span&gt;por [@AUTHOR]&lt;/span&gt;[#ENDIF AUTHOR]&lt;/p&gt;
&lt;/blockquote&gt;[#ENDIF DESCRIPTION]
            </celement>
            <celement id="page-utilities" />
			</pcontrol>

         <!-- CUADRO DE MENSAJE -->
         <pcontrol id="CosmoUiCtrlDynamicMsg" center="true" lateral="false">
            <celement id="info-box-info">
&lt;div class="notice success"&gt;
   &lt;span class="icon medium" data-icon="!" style="display: inline-block;"&gt;&lt;/span&gt;
	[@TEXT] &lt;a href="#close" class="icon close" data-icon="x" style="display: inline-block;"&gt;&lt;/a&gt;
&lt;/div&gt;
            </celement>
            <celement id="info-box-warning">
&lt;div class="notice warning"&gt;
   &lt;span class="icon medium" data-icon="!" style="display: inline-block;"&gt;&lt;/span&gt;
	[@TEXT] &lt;a href="#close" class="icon close" data-icon="x" style="display: inline-block;"&gt;&lt;/a&gt;
&lt;/div&gt;
            </celement>
            <celement id="info-box-error">
&lt;div class="notice error"&gt;
   &lt;span class="icon medium" data-icon="!" style="display: inline-block;"&gt;&lt;/span&gt;
	[@TEXT] &lt;a href="#close" class="icon close" data-icon="x" style="display: inline-block;"&gt;&lt;/a&gt;
&lt;/div&gt;
            </celement>
		   </pcontrol>

         <!-- LISTADO DE OBJETOS -->
         <pcontrol id="CosmoUiCtrlObjectLst" lat-equiv="object-list-lateral">
            <celement id="object-list-title">
[#IF TITLE]&lt;h5&gt;[@TITLE]&lt;/h5&gt;[#ENDIF TITLE]
[#IF DESCRIPTION]&lt;p&gt;[@DESCRIPTION]&lt;/p&gt;[#ENDIF DESCRIPTION]
            </celement>
            <celement id="object-list-header">
&lt;div&gt;
            </celement>
            <celement id="object-list-item">
   &lt;div class="col_2"&gt;&lt;a href="[@HREF]"&gt;&lt;img class="style1" src="[@THUMB]" alt="[@CAPTION]" width="[@WIDTH]" height="[@HEIGHT]" /&gt;&lt;/a&gt;&lt;/div&gt;
      &lt;div class="col_10"&gt;
         &lt;h5 style="margin-top:0px;"&gt;&lt;a href="[@HREF]"&gt;[@CAPTION]&lt;/a&gt;&lt;/h5&gt;
         &lt;p&gt;[@DESCRIPTION]&lt;/p&gt;
         &lt;blockquote class="small" style="margin-bottom:0;"&gt;&lt;span&gt;&lt;span class="icon medium gray" data-icon="i"&gt;&lt;/span&gt; [#IF DATE]Publicado el [@DATE][#IF AUTHOR] por [@AUTHOR][#ENDIF AUTHOR][#ENDIF DATE]&lt;/span&gt;&lt;/blockquote&gt;
      &lt;/div&gt;
   &lt;/div&gt;
   &lt;hr class="alt2" style="margin:5px 0;" /&gt;
            </celement>
            <celement id="object-list-footer">
&lt;/div&gt;
            </celement>
		   </pcontrol>

         <!-- LISTADO DE OBJETOS (BARRAS LATERALES) -->
         <pcontrol id="object-list-lateral">
            <celement id="object-list-title">
[#IF TITLE]&lt;h2&gt;[@TITLE]&lt;/h2&gt;[#ENDIF TITLE]
[#IF DESCRIPTION]&lt;p&gt;[@DESCRIPTION]&lt;/p&gt;[#ENDIF DESCRIPTION]
            </celement>
            <celement id="object-list-header">
&lt;div id="[@OBJID]"&gt;
            </celement>
            <celement id="object-list-item">
   &lt;div class="hlbox"&gt;
      &lt;span class="title"&gt;[@CAPTION]&lt;/span&gt;&lt;br /&gt;
      &lt;a href="[@HREF]"&gt;&lt;img class="thumb" src="[@THUMB]" alt="[@CAPTION]" /&gt;&lt;/a&gt;&lt;br /&gt;
      [@DESCRIPTION]&amp;nbsp;&lt;a href="[@HREF]">Leer&lt;/a&gt;
   &lt;/div&gt;
            </celement>
            <celement id="object-list-footer">
&lt;/div&gt;
            </celement>
         </pcontrol>

         <!-- LISTADO DE CARPETAS -->
         <pcontrol id="folder-list" lat-equiv="folder-list-lateral">
            <celement id="folder-list-title">
&lt;h2&gt;[@TITLE]&lt;/h2&gt;
[#IF DESCRIPTION]&lt;p&gt;[@DESCRIPTION]&lt;/p&gt;[#ENDIF DESCRIPTION]
