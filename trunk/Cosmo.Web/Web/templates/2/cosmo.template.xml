            </celement>
            <celement id="folder-list-footer">
&lt;/ul&gt;
            </celement>
         </pcontrol>

         <!-- TABLA/LISTADO DE DATOS -->
         <pcontrol id="CosmoUiCtrlGrid">
            <celement id="grid-title">
[#IF FTITLE]&lt;h2&gt;[@FTITLE]&lt;/h2&gt;[#ENDIF FTITLE]
[#IF FDESC]&lt;p&gt;[@FDESC]&lt;/p&gt;[#ENDIF FDESC]
            </celement>
            <celement id="grid-body">
&lt;table class="striped tight sortable"&gt;
   &lt;thead&gt;
[@TITLEROW]
   &lt;/thead&gt;
   &lt;tbody&gt;
[@DATAROWS]
   &lt;/tbody&gt;
&lt;/table&gt;
            </celement>
            <celement id="grid-rowtitle-body">&lt;tr&gt;[@CELLS]&lt;/tr&gt;</celement>
            <celement id="grid-rowtitle-cell">&lt;th&gt;[@VALUE]&lt;/th&gt;</celement>
            <celement id="grid-row-body">&lt;tr&gt;[@CELLS]&lt;/tr&gt;</celement>
            <celement id="grid-row-cell">&lt;td&gt;[@VALUE]&lt;/td&gt;</celement>
         </pcontrol>

         <!-- FORMULARIO -->
         <pcontrol id="CosmoUiCtrlForm" separator="&amp;nbsp;&amp;nbsp;" css-class-active="std-button-active" css-class="std-button">
            <cscript type="external" src="include/plugins/jquery/jquery.validate.min.js" />
            <celement id="form-head">
[#IF FTITLE]&lt;h4&gt;[@FTITLE]&lt;/h4&gt;[#ENDIF FTITLE]
[#IF FDESC]&lt;p&gt;[@FDESC]&lt;/p&gt;[#ENDIF FDESC]
[@FTAG]
            </celement>
            <celement id="form-fieldset-head">
&lt;fieldset&gt;
[#IF FTITLE]&lt;legend&gt;[@FTITLE]&lt;/legend&gt;[#ENDIF FTITLE]
[#IF FDESC]&lt;p&gt;[@FDESC]&lt;/p&gt;[#ENDIF FDESC]
            </celement>
            <celement id="form-fieldset-footer">
&lt;/fieldset&gt;
            </celement>
            <celement id="form-control">
&lt;p&gt;&lt;label&gt;[@FLABEL]&lt;br /&gt;[@FCONTROL][#IF FDESC]&lt;br /&gt;&lt;span class="comment"&gt;[@FDESC]&lt;/span&gt;[#ENDIF FDESC]&lt;/label&gt;&lt;/p&gt;
            </celement>
            <celement id="form-option">
&lt;p&gt;
   &lt;label&gt;
[@FCONTROL] [@FLABEL]
[#IF DESC]&lt;br /&gt;&lt;span class="comment"&gt;[@DESC]&lt;/span&gt;[#ENDIF DESC]
   &lt;/label&gt;
&lt;/p&gt;
            </celement>
            <celement id="form-textarea">
&lt;p&gt;
   &lt;label&gt;
