  &lt;button type="button" class="close" data-dismiss="alert"&gt;&amp;times;&lt;/button&gt;
  &lt;h4&gt;Informació:&lt;/h4&gt;
  [@TEXT]
&lt;/div&gt;
            </celement>
            <celement id="info-box-warning">
&lt;div class="alert alert-block"&gt;
  &lt;button type="button" class="close" data-dismiss="alert"&gt;&amp;times;&lt;/button&gt;
  &lt;h4&gt;Atenció:&lt;/h4&gt;
  [@TEXT]
&lt;/div&gt;
