         <!-- SLIDER -->
         <pcontrol id="CosmoUiCtrlSlider">
            <celement id="slider-element-first">active</celement>
            <celement id="slider-header">
&lt;div id="[@ID]" class="carousel slide"&gt;
            </celement>
            <celement id="slider-item-header">
  &lt;ol class="carousel-indicators"&gt;
            </celement>
            <celement id="slider-item">
    &lt;li data-target="#[@ID]" data-slide-to="[@COUNT]" [#IF FIRST]class="[@FIRST]"[#ENDIF FIRST]&gt;&lt;/li&gt;
            </celement>
            <celement id="slider-item-footer">
  &lt;/ol&gt;
            </celement>
            <celement id="slider-content-header">
  &lt;div class="carousel-inner"&gt;
            </celement>
            <celement id="slider-content">
    &lt;div class="item[#IF FIRST] [@FIRST][#ENDIF FIRST]"&gt;
      [@CONTENTS]
      [#IF TITLE]&lt;div class="carousel-caption"&gt;
        &lt;h4&gt;[@TITLE]&lt;/h4&gt;
        [#IF DESCRIPTION]&lt;p&gt;[@DESCRIPTION]&lt;/p&gt;[#ENDIF DESCRIPTION]
      &lt;/div&gt;[#ENDIF TITLE]
    &lt;/div&gt;
            </celement>
            <celement id="slider-content-footer">
  &lt;/div&gt;
            </celement>
            <celement id="slider-footer">
  &lt;a class="carousel-control left" href="#[@ID]" data-slide="prev"&gt;&amp;lsaquo;&lt;/a&gt;
  &lt;a class="carousel-control right" href="#[@ID]" data-slide="next"&gt;&amp;rsaquo;&lt;/a&gt;
&lt;/div&gt;
            </celement>
			</pcontrol>

